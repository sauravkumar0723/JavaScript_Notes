// objects literals
// creating an Objects 
// --> object will be creating of const  ---? object hamesa const ke use se banega

const JsUser = {
    name: "Saurav",
    // [mySym]: "myKey1",
    lastname: "Sharma",
    age: "26",
    hobby: "Developer",
}
// 1st way to Acess Objects but this is not right way
console.log(JsUser.name);
console.log(JsUser.lastname);
console.log(JsUser.age);
console.log(JsUser.hobby);

